{
  "manifest_version": 2,
  "name": "URL Analyzer",
  "version": "1.0",
  "description": "Analyze URLs and display their characteristics",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*"
  ]
}
